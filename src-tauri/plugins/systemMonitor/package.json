{
  "author": "WebArcade Team",
  "description": "System resource monitoring (CPU, RAM, GPU)",
  "name": "systemMonitor",
  "version": "1.0.0",
  "webarcade": {
    "id": "systemMonitor",
    "routes": [
      {
        "handler": "handle_cpu",
        "method": "GET",
        "path": "/cpu"
      },
      {
        "handler": "handle_gpu",
        "method": "GET",
        "path": "/gpu"
      },
      {
        "handler": "handle_memory",
        "method": "GET",
        "path": "/memory"
      },
      {
        "handler": "handle_stats",
        "method": "GET",
        "path": "/stats"
      }
    ]
  }
}