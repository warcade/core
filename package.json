{
  "name": "webarcade",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "High-speed development framework for web and desktop applications",
  "scripts": {
    "build": "node app/scripts/build.js",
    "build:prod": "cross-env NODE_ENV=production node app/scripts/build.js",
    "app": "bun run build:prod && cd app && cargo build --release && cargo packager --release",
    "app:locked": "bun run build:prod && cd app && cargo build --release --features locked-plugins && cargo packager --release",
    "app:run": "cd app/target/release && Emils.exe",
    "cli": "cd cli && cargo run --release",
    "package": "cd cli && cargo run --release -- package",
    "clean": "rm -rf app/dist app/target cli/target",
    "lint": "oxlint",
    "plugin:new": "cd cli && cargo run --release -- new",
    "plugin:build": "cd cli && cargo run --release -- build",
    "plugin:list": "cd cli && cargo run --release -- list",
    "cli:build": "cd cli && cargo build --release"
  },
  "dependencies": {
    "@tabler/icons-solidjs": "^3.34.1",
    "daisyui": "^5.0.50",
    "solid-js": "^1.9.7"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@swc/core": "^1.15.3",
    "@tailwindcss/postcss": "^4.1.12",
    "babel-preset-solid": "^1.9.9",
    "cross-env": "^7.0.3",
    "cssnano": "^7.1.2",
    "esbuild": "^0.27.0",
    "png-to-ico": "^3.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12"
  }
}
